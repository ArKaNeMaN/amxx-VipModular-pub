{
    "Module": "WeaponMenu",

    /* Сколько предметов за раунд можно взять из всех доступных меню */
    "Count": 10,

    /* Проверки перед открытием списка меню */
    "Limits": [
        // Все меню доступны только со второго раунда
        "File:Limits/MinRound-2"
    ],

    /* Проверки перед автооткрытием меню в начале раунда */
    "AutoopenLimits": "File:Limits/Menus/AutoOpen",
    /*
        В данном случае лимиты для авто-открытия одинаковы для всех меню,
        поэтому они вынесены в отдельный файл,
        чтобы можно было легко их менять сразу для всех меню.
    */

    /* Список доступных меню */
    "Menus": [
        // Главному админу доступны сразу обе менюшки
        "File:WeaponMenu/Menus/Vip",
        "File:WeaponMenu/Menus/Premium"
    ]
}