{
    /*
        Тут указываются все возможные варианты
        клиенских команд модуля WeaponMenu
    */

    // Команда открытия меню
    "vipmenu": [
        "vm",
        "say /vm",
        "say_team /vm",
        
        "vipmenu",
        "say /vipmenu",
        "say_team /vipmenu"
    ],
    
    // Команда открытия меню без вывода сообщения в случае недоступности
    // Нужна для внутреннего использования, но можно и снаружи использовать
    "vipmenu_silent": [
        "vm_silent",
        "vipmenu_silent"
    ],
    
    // Команда включения/отключения автооткрытия меню
    // Состояние сбрасывается при выходе с сервера
    "vipmenu_autoopen": [
        "vm_autoopen",
        "say /vm_autoopen",
        "say_team /vm_autoopen",
        
        "vm_ao",
        "say /vm_ao",
        "say_team /vm_ao",
        
        "vipmenu_autoopen",
        "say /vipmenu_autoopen",
        "say_team /vipmenu_autoopen"
    ]
}