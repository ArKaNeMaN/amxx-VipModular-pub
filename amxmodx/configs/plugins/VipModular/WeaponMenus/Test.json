{
    "Module": "WeaponMenu",

    /* Заголовок главного меню. По умолчанию используется MENU_MAIN_TITLE из lang файла */
    "MainMenuTitle": "Оружейки:",

    /* Сколько предметов за раунд можно взять из всех доступных меню */
    "Count": 2,

    /* Проверки перед открытием списка меню */
    "Limits": [
        "File:Limits/MinRound-2"
    ],

    /* Проверки перед автооткрытием меню в начале раунда */
    "AutoopenLimits": [
        { // Авто-открытие меню будет срабатывать только если у игрока нет основного оружия
            "Limit": "HasPrimaryWeapon",
            "HasNot": true
        }
    ],

    /* Список доступных меню */
    "Menus": [
        "File:WeaponMenus/Menus/Test1",
        "File:WeaponMenus/Menus/Test2"
    ]
}
